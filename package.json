{
  "name": "ai-powered-dashboard",
  "version": "1.0.0",
  "description": "AI-Powered Analytics Dashboard with React frontend and Node.js backend",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "start:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && cross-env PORT=5005 npm start",
    "dev": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:backend": "cd backend && npm install",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "react",
    "nodejs",
    "dashboard",
    "analytics",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "openai": "^5.8.3",
    "socket.io": "^4.6.1",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/date-fns": "^2.5.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "typescript": "^5.9.2"
  }
}
